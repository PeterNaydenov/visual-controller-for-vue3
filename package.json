  },
  "devDependencies": {
    "@peter.naydenov/notice": "^2.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "cypress": "^13.6.6",
    "rollup": "^4.13.0",
    "vite": "^5.1.6"
  }
}
